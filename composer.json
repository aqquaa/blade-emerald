{
    "name": "aqua/blade-emerald",
    "description": "Emmet like Abbreviation for Laravel blade component to generate markup",
    "keywords": [
        "HTML",
        "Laravel",
        "Blade Component"
    ],
    "homepage": "https://github.com/devsrv",
    "license": "MIT",
    "authors": [
        {
            "name": "Sourav Rakshit",
            "email": "hello@itsrav.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^7.0 || ^8.0",
        "illuminate/view": "^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Aqua\\Emerald\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aqua\\Emerald\\EmeraldServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
