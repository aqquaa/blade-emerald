{
    "name": "aqua/blade-emerald",
    "description": "Emmet like Abbreviation for Laravel Blade Component to generate & wrap with markup",
    "keywords": [
        "HTML",
        "Laravel",
        "Blade Component",
        "Blade HTML Markup Generate"
    ],
    "homepage": "https://github.com/aqquaa/blade-emerald",
    "license": "MIT",
    "authors": [
        {
            "name": "Sourav Rakshit",
            "email": "hello@itsrav.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^8.0",
        "illuminate/view": "^8.0",
        "spatie/html-element": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Aqua\\Emerald\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aqua\\Emerald\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aqua\\Emerald\\EmeraldServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
